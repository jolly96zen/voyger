{
  "name": "voyger",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "preinstall": "pnpx only-allow pnpm",
    "voy": "pnpm run voy:install && pnpm run dev",
    "voy:remove": "rm -rf .nuxt .pnpm-store node_modules pnpm-lock.yaml",
    "voy:install": "pnpm run voy:remove && pnpm install",
    "voy:check-updates": "pnpx npm-check-updates",
    "voy:upgrade": "pnpx npm-check-updates -u"
  },
