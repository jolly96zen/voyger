{
  "name": "voyger",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "preinstall": "pnpx only-allow pnpm",
    "voyger": "pnpm run voyger:install && pnpm run dev",
    "voyger:remove": "rm -rf .nuxt .pnpm-store node_modules pnpm-lock.yaml",
    "voyger:install": "pnpm run voyger:remove && pnpm install",
    "voyger:check-updates": "pnpx npm-check-updates",
    "voyger:upgrade": "pnpx npm-check-updates -u"
  },
  "dependencies": {
    "nuxt": "3.11.2",
    "vue": "3.4.27",
    "vue-router": "4.3.2"
  },
  "devDependencies": {
    "@nuxt/eslint": "0.3.13",
    "@nuxt/test-utils": "3.13.0",
    "@nuxtjs/supabase": "1.2.2",
    "@pinia-plugin-persistedstate/nuxt": "1.2.0",
    "@pinia/nuxt": "0.5.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.13",
    "@vue/test-utils": "2.4.6",
    "@vueuse/core": "10.9.0",
    "@vueuse/nuxt": "10.9.0",
    "autoprefixer": "10.4.19",
    "daisyui": "4.11.1",
    "eslint-config-prettier": "9.1.0",
    "happy-dom": "14.11.0",
    "html-to-image": "1.11.11",
    "openai": "4.47.1",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "3.2.1",
    "playwright-core": "1.44.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-tailwindcss": "0.5.14",
    "tailwindcss": "3.4.3",
    "vitest": "1.6.0",
    "zod": "3.23.8"
  }
}
