    "@nuxt/image": "1.6.0",
    "@nuxt/test-utils": "3.12.1",
    "@nuxtjs/supabase": "1.2.1",
    "@pinia-plugin-persistedstate/nuxt": "1.2.0",
    "@pinia/nuxt": "0.5.1",
    "@secretlint/secretlint-rule-preset-recommend": "8.2.4",
